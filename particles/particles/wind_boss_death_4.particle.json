{
  "format_version": "1.10.0",
  "particle_effect": {
    "description": {
      "identifier": "wind_boss_death_4",
      "basic_render_parameters": {
        "material": "particles_blend",
        "texture": "textures/particle/wind_boss_death_4"
      }
    },
    "curves": {
      "variable.size1": {
        "type": "bezier",
        "input": "v.particle_age",
        "horizontal_range": "v.particle_lifetime",
        "nodes": [1, 1, 1, 0]
      }
    },
    "components": {
      "minecraft:emitter_rate_steady": {
        "spawn_rate": 30,
        "max_particles": 100
      },
      "minecraft:emitter_lifetime_once": {
        "active_time": 2
      },
      "minecraft:emitter_shape_sphere": {
        "offset": [0, 0.1, 0],
        "radius": "math.random(10,12.5)",
        "surface_only": true,
        "direction": "inwards"
      },
      "minecraft:particle_lifetime_expression": {
        "max_lifetime": "math.random(0.5,0.5)"
      },
      "minecraft:particle_initial_speed": "math.random(25,7.5)",
      "minecraft:particle_motion_dynamic": {
        "linear_drag_coefficient": 1
      },
      "minecraft:particle_appearance_billboard": {
        "size": ["(5*variable.particle_random_1)*variable.size1", "(2.5*variable.particle_random_1)*variable.size1"],
        "facing_camera_mode": "lookat_direction",
        "uv": {
          "texture_width": 16,
          "texture_height": 16,
          "uv": [0, 0],
          "uv_size": [16, 16]
        }
      },
      "minecraft:particle_motion_collision": {
        "collision_radius": 0.1
      },
      "minecraft:particle_appearance_tinting": {
        "color": {
          "interpolant": "v.particle_age / v.particle_lifetime",
          "gradient": {
            "0.0": "#FF000000",
            "0.28": "#FF8900FF",
            "0.6": "#FFC930FF",
            "1.0": "#FF000000"
          }
        }
      }
    }
  }
}
